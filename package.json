{
  "name": "nuxt-app",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "concurrently -k -s first -n web,rag,agent,quiz \"npm:dev:web\" \"npm:dev:rag\" \"npm:dev:agent\" \"npm:dev:quiz\"",
    "dev:web": "HOST=0.0.0.0 nuxt dev --no-fork",
    "dev:rag": "cd server/python-services/rag-service && python main.py",
    "dev:agent": "cd server/python-services/agent-service && python main.py",
    "dev:quiz": "cd server/python-services/quiz-service && python main.py",
    "dev:all": "npm run dev",
    "test:e2e": "playwright test",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.928.0",
    "@aws-sdk/s3-request-presigner": "^3.928.0",
    "bcryptjs": "^3.0.3",
    "cookie-parser": "^1.4.7",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.19.3",
    "multer": "^2.0.2",
    "nuxt": "^3.13.0",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.48.2",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/multer": "^2.0.0",
    "@types/node": "^20.0.0",
    "@types/passport": "^1.0.17",
    "@types/passport-google-oauth20": "^2.0.17",
    "concurrently": "^9.0.0"
  }
}
